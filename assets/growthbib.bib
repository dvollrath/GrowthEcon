	Month = {02},
	Number = {1},
	Pages = {73-117},
	Title = {Disease and Development: Evidence from Hookworm Eradication in the American South},
	Url = {http://ideas.repec.org/a/tpr/qjecon/v122y2007i1p73-117.html},
	Volume = {122},
