	Keywords = {takeoff},
	Number = {4},
	Pages = {pp. 1189-1216},
	Publisher = {American Economic Association},
	Title = {Markets in China and Europe on the Eve of the Industrial Revolution},
	Url = {http://www.jstor.org/stable/30034089},
	Volume = {97},
	Year = {2007},
